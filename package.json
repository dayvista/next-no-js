{
  "name": "next-no-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently 'NODE_ENV=development yarn modfed:watch' 'next dev'",
    "build": "yarn modfed:build:entry && next build",
    "modfed:build:entry": "webpack --json '.next/modfed-entry.json'",
    "modfed:watch": "webpack serve",
    "lint:fix": "prettier '**/*.{ts,tsx,js}' --write",
    "next:build": "next build",
    "start": "next start",
    "export": "next export",
    "clean": "rm -rf .next"
  },
  "author": "shane osboune",
  "dependencies": {
    "@hotwired/turbo": "^7.0.0-beta.4",
    "@types/react": "^17.0.2",
    "@xstate/inspect": "^0.4.1",
    "autoprefixer": "^10.2.4",
    "concurrently": "^6.0.0",
    "esbuild-loader": "^2.9.1",
    "next": "10.0.5",
    "postcss": "^8.2.6",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sync-rpc": "^1.3.6",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5",
    "webpack": "5.21.2",
    "webpack-cli": "^4.5.0",
    "xstate": "^4.16.2"
  },
  "resolutions": {
    "webpack": "5.21.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.11.2"
  }
}
