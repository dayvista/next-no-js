{
  "name": "next-no-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn build:server && TURBO_LINKS=true yarn modfed:build:entry && echo \"BOOTSTRAP=$(node env.js)\nTURBO_LINKS=true\" > .env.local && next build",
    "build:server": "tsc --project tsconfig.server.json",
    "codegen": "npm run codegen:gql:schema && npm run codegen:gql:types && npm run codegen:gql:union",
    "codegen:gql:schema": "bash scripts/gql-schema.bash",
    "codegen:gql:types": "bash scripts/gql-types.bash",
    "codegen:gql:union": "node scripts/gql-union-types.js",
    "clean": "rm -rf .next",
    "_dev": "npm run build:server && PORT=3000 NODE_ENV=development node server-dist/server",
    "dev:all": "concurrently 'NODE_ENV=development yarn modfed:watch' 'yarn _dev'",
    "dev:built": "yarn build && concurrently 'NODE_ENV=development TURBO_LINKS=true yarn modfed:watch' 'PORT=3001 yarn start'",
    "export": "next export",
    "lint:fix": "prettier '**/*.{ts,tsx,js}' --write",
    "modfed:build:entry": "webpack --json './public/modfed-entry.json'",
    "modfed:watch": "webpack serve",
    "next:build": "next build",
    "start": "NODE_ENV=production node server-dist/server"
  },
  "author": "shane osboune",
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@hotwired/turbo": "^7.0.0-beta.4",
    "@types/react": "^17.0.2",
    "@xstate/inspect": "^0.4.1",
    "apollo": "^2.32.5",
    "autoprefixer": "^10.2.4",
    "concurrently": "^6.0.0",
    "envalid": "^7.0.0",
    "esbuild-loader": "^2.9.1",
    "graphql-tag": "^2.11.0",
    "next": "10.0.5",
    "postcss": "^8.2.6",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sync-rpc": "^1.3.6",
    "tailwindcss": "^2.0.3",
    "tiny-invariant": "^1.1.0",
    "typescript": "^4.1.5",
    "webpack": "5.21.2",
    "webpack-cli": "^4.5.0",
    "xstate": "^4.16.2",
    "http-proxy-middleware": "^1.0.6"
  },
  "resolutions": {
    "webpack": "5.21.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.11.2"
  }
}
